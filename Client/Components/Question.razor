@inherits QuestionBase



<div class="Question-Text">
    <p><b>Question @serialNo :</b> @Question.QuestionText</p>
</div>

<div class="contain-options">
    <input type="checkbox" name="q1" value="a" "@Question.CheckBox1" >
    &nbsp;
    <label for="q1a" >@Question.OptionOne</label><br>
    <br>
    <input type="checkbox" name="q1" value="b" "@Question.CheckBox2">
    &nbsp;
    <label for="q1b"> @Question.OptionTwo</label><br>
    <br>
    <input type="checkbox" name="q1" value="c" "@Question.CheckBox3">
    &nbsp;
    <label for="q1c"> @Question.OptionThree</label><br>
    <br>
    <input type="checkbox" name="q1" value="d" "@Question.CheckBox4">
    &nbsp;
    <label for="q1d">@Question.OptionFour</label><br>
</div>